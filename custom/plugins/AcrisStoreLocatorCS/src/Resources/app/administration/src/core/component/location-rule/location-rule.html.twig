{% block sw_condition_value_content %}
    <div class="sw-condition-location-is-one-of sw-condition__condition-value">

        {% block sw_condition_location_rule_field_operator %}
            <sw-condition-operator-select
                    v-bind="{ operators, condition }"
                    :disabled="disabled"
            />
        {% endblock %}

        {% block sw_condition_location_rule_field_identifiers %}
            <sw-entity-multi-select
                size="medium"
                :disabled="disabled"
                entity="acris_store_locator"
                :entity-collection="locations"
                @change="setLocationIds">
            </sw-entity-multi-select>
        {% endblock %}
    </div>
{% endblock %}
