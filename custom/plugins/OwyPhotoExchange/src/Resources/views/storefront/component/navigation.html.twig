{% block component_navigation %}
								{% set lang = page.metaInformation.xmlLang %}
								{% if lang != 'de-DE' %}
								{% set lang = '/' ~ lang|slice(0,2)|lower %}
								{% else %}
								{% set lang = '' %}
								{% endif %}
	<div class="cms-block  pos-1 cms-block-owy-shoppage-nav" style="">
		<div class="cms-block-container" style="padding: 20px 0 0 0;">
			<div class="cms-block-container-row row cms-row has--sidebar">
				<div id="navigation" class="w-100 pl-sm-4 pl-lg-4 pr-sm-4 pr-lg-4">
					<ul class="category-navigation level">
						<li class="category-navigation-entry is--parentactive">
							<a class="category-navigation-link is-active" href="">
								{{ "OWY.photo_exchange.title"|trans }}
							</a>
							<ul class="category-navigation level">
								<li class="category-navigation-entry ">
									<a class="category-navigation-link is-listpage" href="{{ lang }}/photo-exchange/list">
										{{ "OWY.photo_exchange.show"|trans }}
									</a>
								</li>
								<li class="category-navigation-entry ">
									<a class="category-navigation-link is-serachpage" href="{{ lang }}/photo-exchange/search">
										{{ "OWY.photo_exchange.search"|trans }} 
									</a>
								</li>
								<li class="category-navigation-entry ">
									<a class="category-navigation-link is-createpage" href="{{ lang }}/photo-exchange/create">
										{{ "OWY.photo_exchange.create"|trans }}
									</a>
								</li>
							</ul>
						</li>
					</ul>
				</div>
			</div>
		</div>
	</div>
{% endblock %}
