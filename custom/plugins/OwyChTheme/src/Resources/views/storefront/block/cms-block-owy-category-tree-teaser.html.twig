{% block block_owy_category_tree_teaser %}
        {% if page.header.extensions.navigationTree.getNavigationTree().getTree() %}
            {% set element = block.slots.getSlot('content') %}
            <div class="w-100 pl-sm-4 pl-lg-4 pr-sm-4 pr-lg-4"  data-cms-element-id="{{ element.id }}">
                {% sw_include "@OwyChTheme/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            </div>
        {% endif %}
{% endblock %}
