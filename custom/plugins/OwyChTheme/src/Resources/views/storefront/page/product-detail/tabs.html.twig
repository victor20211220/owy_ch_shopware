							<img src="{{ objektivkonstruktionImg }}" alt="objektivkonstruktionImg" class="img-fluid">
						{% endif %}
					</div>
					<div class="col-md-12 graphbox
													">
						{% if leistungsmerkmaletitle %}
							<h3>{{ leistungsmerkmaletitle }}</h3>
						{% endif %}
						<div class="row">
							<div class="col-md-6">
								{% if leistungsmerkmaleheading1 %}
									<label>{{ leistungsmerkmaleheading1 }}</label>
								{% endif %}
								{% if leistungsmerkmaleImage1 %}
									{% set mediaCollection = searchMedia([leistungsmerkmaleImage1], context.context) %}
									{% set leistungsmerkmaleImg1 = mediaCollection.get(leistungsmerkmaleImage1).url %}
									<img class="img-fluid" src="{{ leistungsmerkmaleImg1 }}" alt="leistungsmerkmale Image">
								{% endif %}
							</div>
							<div class="col-md-6">
								{% if leistungsmerkmaleheading2 %}
									<label>{{ leistungsmerkmaleheading2 }}</label>
								{% endif %}
								{% if leistungsmerkmaleImage2 %}
									{% set mediaCollection = searchMedia([leistungsmerkmaleImage2], context.context) %}
