{% set portraitshootingImage = page.product.customFields.custom_product_detail_portraitshooting_img %}
{% set portraitshootingDesc = page.product.customFields.custom_product_detail_portraitshooting_desc %}
{% if portraitshootingImage || portraitshootingDesc %}
	<div id="section3" class="detailsections">
		<div class="container">
			<div class="infocontent">
				<div class="row ">
					{% if portraitshootingImage %}
						{% set mediaCollection = searchMedia([portraitshootingImage], context.context) %}
						{% set portraitshootingImg = mediaCollection.get(portraitshootingImage).url %}
						<div class="col-md-6">
							<img src="{{ portraitshootingImg }}" alt="Tabs Information">
						</div>
					{% endif %}
					{% if portraitshootingDesc %}
						<div class="col-md-6">
							{{ portraitshootingDesc|raw }}
						</div>
					{% endif %}
				</div>
			</div>
		</div>
	</div>
{% endif %}

{% set aubergewohnlicheImage = page.product.customFields.custom_product_detail_aubergewohnliche_img %}
{% set aubergewohnlicheDesc = page.product.customFields.custom_product_detail_aubergewohnliche_desc %}
{% if aubergewohnlicheImage || aubergewohnlicheDesc %}
	<div id="section4" class="detailsections">
		<div class="container">
			<div class="infocontent">
				<div class="row">
					<div class="col-md-6">
						{{ aubergewohnlicheDesc|raw }}
					</div>
					{% if aubergewohnlicheImage %}
						{% set mediaCollection = searchMedia([aubergewohnlicheImage], context.context) %}
						{% set aubergewohnlicheImg = mediaCollection.get(aubergewohnlicheImage).url %}
						<div class="col-md-6">
							<img src="{{ aubergewohnlicheImg }}" alt="Tabs Information" class="img-fluid">
						</div>
					{% endif %}
				</div>
			</div>
		</div>
	</div>
{% endif %}

{% if page.product.sortedProperties|length > 0 %}
	<div id="section5" class="detailsections techcontainer">
		<div class="container">
			<div class="infocontent">
				<div class="techdata">{{ "OwyChTheme.detail.tabsSections.properties"|trans }}</div>
				{% sw_include '@Storefront/storefront/page/product-detail/properties.html.twig' %}
			</div>
		</div>
	</div>
{% endif %}

{% set objektivkonstruktiontitle = page.product.customFields.custom_product_detail_objektivkonstruktion_title %}
{% set objektivkonstruktionImage = page.product.customFields.custom_product_detail_objektivkonstruktion_img %}
