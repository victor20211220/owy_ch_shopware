                            v-model="element.config.award3title1.value"
                            label="Heading"
                    />
                    <sw-textarea-field
                            v-model="element.config.award3desc.value"
                            type="textarea"
                            label="Description"
                    >
                    </sw-textarea-field>

                    <sw-media-field required v-model="element.config.award3image2.value"
                                    label="Upload Image 2"/>

                    <sw-text-field
                            v-model="element.config.award3title3.value"
                            label="Award date"
                    />
                    <sw-text-field
                            v-model="element.config.award3title4.value"
                            label="Award Title"
                    />
                    <sw-url-field
                            label="Link 3"
                            v-model="element.config.award3link3.value"
                            placeholder="Link 3"
                            :error="null"
                            :omitUrlHash="false"
                            :omitUrlSearch="false"
                    >
                    </sw-url-field>

                </sw-container>
            {% endblock %}

            {% block sw_cms_el_owy_awards_widget_config_tab5 %}
                <sw-container v-if="active === 'tab5'" class="sw-cms-el-config-owy_awards_widget__tab-tab5">
                    <sw-media-field required v-model="element.config.award4image1.value" label="Upload Image"/>
                    <sw-text-field
                            v-model="element.config.award4title1.value"
                            label="Heading"
                    />
                    <sw-textarea-field
                            v-model="element.config.award4desc.value"
                            type="textarea"
                            label="Description"
                    >
                    </sw-textarea-field>

                    <sw-media-field required v-model="element.config.award4image2.value"
                                    label="Upload Image 2"/>

                    <sw-text-field
                            v-model="element.config.award4title3.value"
                            label="Award date"
                    />
                    <sw-text-field
                            v-model="element.config.award4title4.value"
                            label="Award Title"
                    />

                    <sw-url-field
                            label="Link 4"
                            v-model="element.config.award4link4.value"
                            placeholder="Link 4"
                            :error="null"
                            :omitUrlHash="false"
                            :omitUrlSearch="false"
                    >
                    </sw-url-field>

                </sw-container>
            {% endblock %}

            {% block sw_cms_el_owy_awards_widget_config_tab6 %}
                <sw-container v-if="active === 'tab6'" class="sw-cms-el-config-owy_awards_widget__tab-tab6">
                    <sw-media-field required v-model="element.config.award5image1.value" label="Upload Image"/>
                    <sw-text-field
                            v-model="element.config.award5title1.value"
                            label="Heading"
                    />
                    <sw-textarea-field
                            v-model="element.config.award5desc.value"
                            type="textarea"
                            label="Description"
                    >
                    </sw-textarea-field>

                    <sw-media-field required v-model="element.config.award5image2.value"
                                    label="Upload Image 2"/>

                    <sw-text-field
                            v-model="element.config.award5title3.value"
                            label="Award date"
                    />
                    <sw-text-field
                            v-model="element.config.award5title4.value"
                            label="Award Title"
                    />

                    <sw-url-field
                            label="Link 5"
                            v-model="element.config.award5link5.value"
                            placeholder="Link 5"
                            :error="null"
                            :omitUrlHash="false"
                            :omitUrlSearch="false"
                    >
                    </sw-url-field>

                </sw-container>
            {% endblock %}
        </template>
    </sw-tabs>

{% endblock %}
