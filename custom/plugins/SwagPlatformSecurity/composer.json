{
    "name": "swag/platform-security",
    "version": "2.0.1",
    "description": "Fixes known issues for outdated Shopware installations",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "shopware AG"
        }
    ],
    "require": {
        "shopware/core": "~6.5.0"
    },
    "autoload": {
        "psr-4": {
            "Swag\\Security\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swag\\Security\\Tests\\": "tests/"
        }
    },
    "extra": {
        "shopware-plugin-class": "Swag\\Security\\SwagPlatformSecurity",
        "label": {
            "de-DE": "Shopware 6 Sicherheits-Plugin",
            "en-GB": "Shopware 6 Security Plugin"
        },
        "description": {
            "de-DE": "Schütze Deinen Onlineshop vor Sicherheitslücken - mit diesem Plugin kannst Du mit einem einfachen Plugin-Update Deinen Shop schnell und einfach absichern.",
            "en-GB": "Protect your online shop from security gaps - with this Shopware 6 security plugin you can quickly and easily secure your shop with a simple plugin update."
        },
        "manufacturerLink": {
            "de-DE": "https://store.shopware.com/shopware-ag.html",
            "en-GB": "https://store.shopware.com/en/shopware-ag.html"
        },
        "supportLink": {
            "de-DE": "https://docs.shopware.com/de",
            "en-GB": "https://docs.shopware.com/en"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
